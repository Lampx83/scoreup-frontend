{
  "name": "my-project",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "start": "parcel src/html/*.html --no-cache",
    "build": "parcel build --public-url /exam src/html/*.html",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "git add"
    ],
    "**/*.html": [
      "htmlhint",
      "git add"
    ],
    "**/*.css": [
      "stylelint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "buffer": "^6.0.3",
    "eslint": "^9.0.0",
    "globals": "^15.0.0",
    "htmlhint": "^1.1.4",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "parcel": "^2.10.0",
    "process": "^0.11.10",
    "stylelint": "^16.3.1",
    "stylelint-config-standard": "^36.0.0"
  },
  "dependencies": {
    "@parcel/transformer-sass": "^2.10.0",
    "@parcel/transformer-vue": "^2.10.0",
    "axios": "^1.5.1",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.2",
    "jquery": "^3.7.1",
    "medium-editor": "^5.23.3",
    "owl.carousel": "^2.3.4",
    "prettier": "^3.3.2",
    "rangy": "^1.3.1",
    "viewerjs": "^1.11.6",
    "vue": "^3.3.4"
  }
}
